<ul>
    {{#if item.data.providedItems}}

    {{/if}}
    {{#unless item.data.providedItems}}
        {{#each item.data._source.data.providedItems as |item id|}}
            <li class="flexrow">
                <div>{{item.name}}</div>
                <div>{{#if item.prerequisite}} Prerequisite:{{item.prerequisite.text}}{{/if}}</div>
            </li>
        {{/each}}
    {{/unless}}
</ul>